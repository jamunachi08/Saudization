  },
  {
   "fieldname": "nationality_group",
   "label": "Nationality Group",
   "fieldtype": "Select",
   "options": "Saudi\nGCC\nNon-GCC\nUnknown",
   "depends_on": "eval:doc.dimension_type==\"Nationality Group\"",
   "in_list_view": 1
  },
  {
   "fieldname": "target_percent",
   "label": "Target %",
   "fieldtype": "Percent",
   "reqd": 1,
   "in_list_view": 1
  },
  {
   "fieldname": "min_headcount",
   "label": "Minimum Headcount",
