   "in_list_view": 1
  },
  {
   "fieldname": "designation",
   "label": "Designation",
   "fieldtype": "Link",
   "options": "Designation",
   "depends_on": "eval:doc.dimension_type==\"Designation\" || doc.dimension_type==\"Department+Designation\"",
