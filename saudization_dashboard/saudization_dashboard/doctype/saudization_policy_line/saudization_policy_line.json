{
 "doctype": "DocType",
 "name": "Saudization Policy Line",
 "module": "Saudization Dashboard",
 "custom": 0,
 "istable": 1,
 "editable_grid": 1,
 "sort_field": "idx",
 "sort_order": "ASC",
 "fields": [
